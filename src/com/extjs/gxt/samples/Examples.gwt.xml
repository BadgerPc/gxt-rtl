<module>
<inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name="com.google.gwt.i18n.CldrLocales"/>
  <inherits name='com.extjs.gxt.samples.resources.Resources' />
  <inherits name='com.extjs.gxt.themes.Themes' />
  <inherits name='com.extjs.gxt.charts.Chart' />
  <servlet path="/service" class="com.extjs.gxt.samples.server.ExampleServiceImpl"/>
  <servlet path="/fileservice" class="com.extjs.gxt.samples.server.FileServiceImpl"/>
  <entry-point class='com.extjs.gxt.samples.client.Examples' />
  
   <!-- English language, independent of country -->
	<extend-property name="locale" values="en" />

	<!-- Persian -->
	<extend-property name="locale" values="fa_IR" />
</module>